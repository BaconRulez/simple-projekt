{
    "document": {
        "head": {
            "meta": [
                {
                    "charset": "UTF-8"
                },
                {
                    "name": "viewport",
                    "content": "width=device-width, initial-scale=1.0"
                }
            ],
            "title": "Our Products - Simple Shop",
            "link": {
                "rel": "stylesheet",
                "href": "style.css"
            }
        },
        "body": {
            "header": {
                "h1": "Our Products",
                "nav": {
                    "link": [
                        {
                            "href": "/home",
                            "text": "Home"
                        },
                        {
                            "href": "/products",
                            "class": "active",
                            "text": "Products"
                        },
                        {
                            "href": "/cart",
                            "text": "Cart"
                        },
                        {
                            "href": "/",
                            "text": "Log Out"
                        }
                    ]
                }
            },
            "main": {
                "section": {
                    "class": "product-list",
                    "h2": "Explore Our Collection",
                    "p": "Browse through our top-rated products and add them to your cart.",
                    "product-grid": {
                        "product": [
                            {
                                "data-price": "50",
                                "img": {
                                    "src": "/img/flashdrive.jpg",
                                    "alt": "USB Flash Drive"
                                },
                                "h3": "U Disk USB Flash Drive",
                                "description": "Compact and stylish JetFlash 300. Gloss-black finish with a sleek design.",
                                "price": "$10",
                                "button": {
                                    "onclick": "addToCart('USB Flash Drive', 10, '/img/flashdrive.jpg')",
                                    "text": "Add to Cart"
                                }
                            },
                            {
                                "data-price": "15",
                                "img": {
                                    "src": "/img/head.jpg",
                                    "alt": "B39 LED Wireless Bluetooth Headphone"
                                },
                                "h3": "B39 LED Wireless Bluetooth Headphone",
                                "description": "Wireless Bluetooth 5.0 headset with mic, noise cancellation, and a foldable design.",
                                "price": "$15",
                                "button": {
                                    "onclick": "addToCart('Bluetooth Headphone', 15, '/img/head.jpg')",
                                    "text": "Add to Cart"
                                }
                            },
                            {
                                "data-price": "20",
                                "img": {
                                    "src": "/img/mouse.jpg",
                                    "alt": "Wireless Optical Gaming Mouse"
                                },
                                "h3": "Wireless Optical Gaming Mouse",
                                "description": "Ergonomic design with adjustable DPI, wireless connectivity, and optical precision.",
                                "price": "$20",
                                "button": {
                                    "onclick": "addToCart('Wireless Optical Gaming Mouse', 20, '/img/mouse.jpg')",
                                    "text": "Add to Cart"
                                }
                            }
                        ]
                    }
                }
            },
            "script": {
                "src": "script.js"
            }
        }
    }
}